tab4:Checkbox({
	Title    = "Instant Interact";
	Callback = function(value)
if value == true then
	local CheckT
for i,v in pairs(getgc(true)) do 
    if typeof(v) == "table" then 
        if rawget(v, "ToolDropCheck") and rawget(v, "CashDropCheck") and rawget(v, "GetExtrasCheck") and rawget(v, "DoorCheck") then 
            local ToolDropCheck  = rawget(v, "ToolDropCheck")
            local CashDropCheck  = rawget(v, "CashDropCheck")
            local GetExtrasCheck = rawget(v, "GetExtrasCheck")
            local DoorCheck      = rawget(v, "DoorCheck")
            
            rawset(v, "ToolDropCheck", function(...)
                local args = {...}
                local tableT, numberN = ToolDropCheck(unpack(args))    
                
                if tableT then
                    tableT[1][2] = 0
                end
                
                return tableT, numberN 
            end)
            rawset(v, "CashDropCheck", function(...)
                local args = {...}
                local tableT, numberN = CashDropCheck(unpack(args))    
                
                if tableT then
                    tableT[1][2] = 0
                end
                
                return tableT, numberN 
            end)
            rawset(v, "GetExtrasCheck", function(...)
                local args = {...}
                local tableT, numberN = GetExtrasCheck(unpack(args))    
                
                if tableT then
                    tableT[1][2] = 0
                end
                
                return tableT, numberN 
            end)
            rawset(v, "DoorCheck", function(...)
                local args = {...}
                local tableT, numberN = DoorCheck(unpack(args))    
                
                if tableT then
                    if tableT[1] then 
                        tableT[1][2] = 0
                    end
                    if tableT[2] then 
                        tableT[2][2] = 0
                    end
                end
                
                return tableT, numberN 
            end)
        end
    end
end
elseif value == false then
	local CheckT
for i,v in pairs(getgc(true)) do 
    if typeof(v) == "table" then 
        if rawget(v, "ToolDropCheck") and rawget(v, "CashDropCheck") and rawget(v, "GetExtrasCheck") and rawget(v, "DoorCheck") then 
            local ToolDropCheck  = rawget(v, "ToolDropCheck")
            local CashDropCheck  = rawget(v, "CashDropCheck")
            local GetExtrasCheck = rawget(v, "GetExtrasCheck")
            local DoorCheck      = rawget(v, "DoorCheck")
            
            rawset(v, "ToolDropCheck", function(...)
                local args = {...}
                local tableT, numberN = ToolDropCheck(unpack(args))    
                
                if tableT then
                    tableT[1][2] = 1
                end
                
                return tableT, numberN 
            end)
            rawset(v, "CashDropCheck", function(...)
                local args = {...}
                local tableT, numberN = CashDropCheck(unpack(args))    
                
                if tableT then
                    tableT[1][2] = 0.5
                end
                
                return tableT, numberN 
            end)
            rawset(v, "GetExtrasCheck", function(...)
                local args = {...}
                local tableT, numberN = GetExtrasCheck(unpack(args))    
                
                if tableT then
                    tableT[1][2] = 0.5
                end
                
                return tableT, numberN 
            end)
            rawset(v, "DoorCheck", function(...)
                local args = {...}
                local tableT, numberN = DoorCheck(unpack(args))    
                
                if tableT then
                    if tableT[1] then 
                        tableT[1][2] = 0
                    end
                    if tableT[2] then 
                        tableT[2][2] = 0
                    end
                end
                
                return tableT, numberN 
            end)
        end
    end
end
end
end;
})




